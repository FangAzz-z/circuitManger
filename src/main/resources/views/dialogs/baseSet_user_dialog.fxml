<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.xbky.circuitManger.view.baseSetUser.BaseSetUserAddController">
    <children>
        <VBox prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <BorderPane prefHeight="350.0" prefWidth="600.0">
                    <center>
                        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                            <children>
                                <TextField fx:id="tfId" prefHeight="0" prefWidth="0" promptText="id" visible="false" />
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label text="姓名">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin></Label>
                                        <TextField fx:id="tfName" prefHeight="30.0" prefWidth="246.0" promptText="姓名">
                                            <VBox.margin>
                                                <Insets left="25.0" right="25.0" />
                                            </VBox.margin></TextField>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="100" prefWidth="200" spacing="15.0">
                                    <children>
                                        <RadioButton fx:id="rbMale" accessibleText="男" mnemonicParsing="false" selected="true" text="男">
                                            <toggleGroup>
                                                <ToggleGroup fx:id="gender" />
                                            </toggleGroup>
                                        </RadioButton>
                                        <RadioButton fx:id="rbFemale" accessibleText="女" mnemonicParsing="false" text="女" toggleGroup="$gender">
                                            <HBox.margin>
                                                <Insets left="10.0" />
                                            </HBox.margin>
                                        </RadioButton>
                                    </children>
                                    <VBox.margin>
                                        <Insets left="25.0" right="25.0" />
                                    </VBox.margin>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label text="部门">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin></Label>
                                        <TextField fx:id="tfDepartment" prefHeight="30.0" prefWidth="246.0" promptText="部门">
                                            <VBox.margin>
                                                <Insets left="25.0" right="25.0" />
                                            </VBox.margin></TextField>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label text="职位">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin></Label>
                                        <TextField fx:id="tfJob" prefHeight="30.0" prefWidth="246.0" promptText="职位">
                                            <VBox.margin>
                                                <Insets left="25.0" right="25.0" />
                                            </VBox.margin></TextField>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label text="电话">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin></Label>
                                        <TextField fx:id="tfPhone" prefHeight="30.0" prefWidth="246.0" promptText="电话">
                                            <VBox.margin>
                                                <Insets left="25.0" right="25.0" />
                                            </VBox.margin></TextField>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </center>
                </BorderPane>
                <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#cancelSubmit" text="取消">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin></Button>
                        <Button mnemonicParsing="false" onAction="#submitData" text="添加" />
                    </children>
                    <VBox.margin>
                        <Insets right="30.0" />
                    </VBox.margin>
                </HBox>
            </children>
         <padding>
            <Insets bottom="10.0" left="10.0" />
         </padding>
        </VBox>
    </children>
    <stylesheets>
        <URL value="@/css/theme/helpers.css" />
        <URL value="@/css/theme/typographic.css" />
        <URL value="@/css/theme/material-color.css" />
        <URL value="@/css/theme/master.css" />
        <URL value="@/css/theme/shape.css" />
        <URL value="@/css/theme/light.css" />
        <URL value="@/css/theme/bootstrap.css" />
        <URL value="@/css/theme/skeleton.css" />
    </stylesheets>
</AnchorPane>
