<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.xbky.circuitManger.view.baseSetInfo.BaseSetInfoAddController">
    <children>
        <VBox prefHeight="300.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <BorderPane prefHeight="300.0" prefWidth="600.0">
                    <center>
                        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                            <children>
                                <HBox prefHeight="0" prefWidth="0">
                                    <children>
                                        <TextField fx:id="tfId" prefHeight="0" prefWidth="0" promptText="id" visible="false" />
                                        <!-- <TextField fx:id="tfType" prefHeight="0" prefWidth="0" promptText="id" visible="false"/>-->
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label fx:id="dialog_label_code" text="故障代码">
                                            <HBox.margin>
                                                <Insets right="10.0" />
                                            </HBox.margin>
                                        </Label>
                                        <TextField fx:id="dialog_code" prefHeight="30.0" prefWidth="246.0" promptText="故障代码">
                                            <VBox.margin>
                                                <Insets left="25.0" right="25.0" />
                                            </VBox.margin>
                                        </TextField>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label fx:id="dialog_label" text="中文释义">
                                            <HBox.margin>
                                                <Insets right="10.0" />
                                            </HBox.margin>
                                        </Label>
                                        <TextField fx:id="dialog_content" prefHeight="30.0" prefWidth="246.0" promptText="中文释义">
                                            <VBox.margin>
                                                <Insets left="25.0" right="25.0" />
                                            </VBox.margin>
                                        </TextField>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </center>
                    <bottom>
                        <HBox alignment="CENTER_RIGHT" prefHeight="44.0" prefWidth="590.0" BorderPane.alignment="CENTER">
                            <children>
                                <Button mnemonicParsing="false" onAction="#cancelSubmit" text="取消">
                                    <HBox.margin>
                                        <Insets right="10.0" />
                                    </HBox.margin>
                                </Button>
                                <Button mnemonicParsing="false" onAction="#submitDataV2" text="添加" />
                            </children>
                            <BorderPane.margin>
                                <Insets right="30.0" />
                            </BorderPane.margin>
                        </HBox>
                    </bottom>
                </BorderPane>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" />
            </padding>
        </VBox>
    </children>
    <stylesheets>
        <URL value="@/css/theme/helpers.css" />
        <URL value="@/css/theme/typographic.css" />
        <URL value="@/css/theme/material-color.css" />
        <URL value="@/css/theme/master.css" />
        <URL value="@/css/theme/shape.css" />
        <URL value="@/css/theme/light.css" />
        <URL value="@/css/theme/bootstrap.css" />
        <URL value="@/css/theme/skeleton.css" />
    </stylesheets>
</AnchorPane>
