<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.xbky.circuitManger.view.baseSetFitting.BaseSetFittingAddController">
    <children>
        <VBox prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <BorderPane prefHeight="350.0" prefWidth="600.0">
                    <center>
                        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                            <children>
                                <TextField fx:id="tfId" prefHeight="0" prefWidth="0" promptText="id" visible="false" />
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label text="配件编号">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin></Label>
                                        <TextField fx:id="tfNo" prefHeight="30.0" prefWidth="246.0" promptText="配件编号">
                                            <VBox.margin>
                                                <Insets left="25.0" right="25.0" />
                                            </VBox.margin></TextField>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label text="配件名称">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin></Label>
                                        <TextField fx:id="tfName" prefHeight="30.0" prefWidth="246.0" promptText="配件名称">
                                            <VBox.margin>
                                                <Insets left="25.0" right="25.0" />
                                            </VBox.margin></TextField>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label text="配件型号">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin></Label>
                                        <TextField fx:id="tfModel" prefHeight="30.0" prefWidth="246.0" promptText="配件型号">
                                            <VBox.margin>
                                                <Insets left="25.0" right="25.0" />
                                            </VBox.margin></TextField>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label text="厂家    ">
                                            <HBox.margin>
                                                <Insets right="10.0" />
                                            </HBox.margin></Label>
                                        <TextField fx:id="tfFactory" prefHeight="30.0" prefWidth="246.0" promptText="单位">
                                            <VBox.margin>
                                                <Insets left="25.0" right="25.0" />
                                            </VBox.margin></TextField>
                                    </children>
                                </HBox>
                                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label text="单位    ">
                                 <HBox.margin>
                                    <Insets right="10.0" />
                                 </HBox.margin></Label>
                                        <TextField fx:id="tfUnit" prefHeight="30.0" prefWidth="246.0" promptText="单位">
                                            <VBox.margin>
                                                <Insets left="25.0" right="25.0" />
                                            </VBox.margin></TextField>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </center>
               <top>
                  <HBox prefHeight="10.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
               </top>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin>
                </BorderPane>
                <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#cancelSubmit" text="取消">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin></Button>
                        <Button mnemonicParsing="false" onAction="#submitData" text="添加" />
                    </children>
                    <VBox.margin>
                        <Insets right="10.0" />
                    </VBox.margin>
               <padding>
                  <Insets right="20.0" />
               </padding>
                </HBox>
            </children>
        </VBox>
    </children>
    <stylesheets>
        <URL value="@/css/theme/helpers.css" />
        <URL value="@/css/theme/typographic.css" />
        <URL value="@/css/theme/material-color.css" />
        <URL value="@/css/theme/master.css" />
        <URL value="@/css/theme/shape.css" />
        <URL value="@/css/theme/light.css" />
        <URL value="@/css/theme/bootstrap.css" />
        <URL value="@/css/theme/skeleton.css" />
    </stylesheets>
</AnchorPane>
